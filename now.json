{
  "version": 2,
  "name": "cherie-api",
  "builds": [
    {
      "src": "public/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      },
      "src": "/.*",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3500",
    "MONGODB_URI": "mongodb://localhost/cherie"
  }
}